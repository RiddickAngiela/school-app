<div id="<%= dom_id event %>">
  <p>
    <strong>Name:</strong>
    <%= event.name %>
  </p>

  <p>
    <strong>Venue:</strong>
    <%= event.venue %>
  </p>

  <p>
    <strong>Time:</strong>
    <%= event.time %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= event.description %>
  </p>

  <p>
    <strong>Members involved:</strong>
    <%= event.members_involved %>
  </p>

  <p>
    <strong>Sponsors:</strong>
    <%= event.sponsors %>
  </p>

</div>
